<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.sardordev.foodreciepts.screens.mainscreen.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_beefFragment"
            app:destination="@id/beefFragment" />
    </fragment>
    <fragment
        android:id="@+id/beefFragment"
        android:name="com.sardordev.foodreciepts.screens.beefscreen.BeefFragment"
        android:label="fragment_beef"
        tools:layout="@layout/fragment_beef" />
</navigation>